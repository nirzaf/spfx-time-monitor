{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "vacation-request-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/vacationRequest/VacationRequestWebPart.js",
          "manifest": "./src/webparts/vacationRequest/VacationRequestWebPart.manifest.json"
        }
      ]
    },
    "vacation-rc-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/vacationRc/VacationRcWebPart.js",
          "manifest": "./src/webparts/vacationRc/VacationRcWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "VacationRequestWebPartStrings": "lib/webparts/vacationRequest/loc/{locale}.js",
    "VacationRcWebPartStrings": "lib/webparts/vacationRc/loc/{locale}.js"
  }
}
